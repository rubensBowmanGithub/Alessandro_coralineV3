      <directory>${basedir}/src/main</directory>
      <outputDirectory>/</outputDirectory>
      <includes>
        <include>bin/*</include>
      </includes>
      <fileMode>0755</fileMode>
    </fileSet>

    <!-- initd -->
    <fileSet>
      <directory>${basedir}/src/main</directory>
      <outputDirectory>/</outputDirectory>
      <includes>
        <include>initd/*</include>
      </includes>
      <excludes>
        <exclude>README</exclude>
        <exclude>initd/_*</exclude>
      </excludes>
      <fileMode>0755</fileMode>
    </fileSet>
    <fileSet>
      <directory>${basedir}/src/main</directory>
      <outputDirectory>/</outputDirectory>
