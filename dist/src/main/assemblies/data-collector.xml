  <includeBaseDirectory>true</includeBaseDirectory>

  <files>
    <file>
      <source>${basedir}/../container/src/main/resources/cor-log4j.properties-default</source>
      <outputDirectory>etc</outputDirectory>
      <destName>cor-log4j.properties</destName>
      <filtered>true</filtered>
      <fileMode>0644</fileMode>
    </file>
  </files>

